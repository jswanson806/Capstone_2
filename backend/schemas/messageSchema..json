{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "message"
    ],
    "properties": {
        "message": {
            "type": "object",
            "default": {},
            "title": "The message Schema",
            "required": [
                "conversation_id",
                "sent_by",
                "sent_to",
                "created_at"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                },
                "conversation_id": {
                    "type": "string",
                    "default": "",
                    "title": "The conversation_id Schema",
                    "examples": [
                        "u1u2"
                    ]
                },
                "sent_by": {
                    "type": "integer",
                    "default": 0,
                    "title": "The sent_by Schema",
                    "examples": [
                        1
                    ]
                },
                "sent_to": {
                    "type": "integer",
                    "default": 0,
                    "title": "The sent_to Schema",
                    "examples": [
                        2
                    ]
                },
                "created_at": {
                    "type": "string",
                    "default": "",
                    "title": "The created_at Schema",
                    "examples": [
                        "2023-06-01 09:00:00"
                    ]
                }
            },
            "examples": [{
                "id": 1,
                "conversation_id": "u1u2",
                "sent_by": 1,
                "sent_to": 2,
                "created_at": "2023-06-01 09:00:00"
            }]
        }
    },
    "examples": [{
        "message": {
            "id": 1,
            "conversation_id": "u1u2",
            "sent_by": 1,
            "sent_to": 2,
            "created_at": "2023-06-01 09:00:00"
        }
    }]
}